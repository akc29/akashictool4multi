    "@akashic-extension/resolve-player-info": "~1.0.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.13",
    "@types/node": "14.10.1",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.9.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "26.4.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.3.0",
    "typedoc": "^0.19.2",
    "typescript": "3.9.7"
